(new model m)
;=>m

(add-node m name1:'(3))
;=>true

(add-constraint m name1:'(?x ?y ?) (uniq on '(?x ?y)))
;=>true

(add-constraint m name1:'(3) (uniq on '(?2 ?1)))
;=>true

(remove-constraint m name1:'(3) (uniq on '(? ?1)))
;=>'((uniq on '(?0 ?1)) (uniq on '(?1 ?2)))

(list-constraint m name1:'(?...))
;=>'()

(list-stmt m from $cst_node$:'(?...))
;=>'()

