(new model m)
;=>m

(add-constraint m n1:'(?x) (max 10 on ?x))
;=>1

(add-stmt m n1:'(1))
;=>true

(add-stmt m n1:'(10))
;=>true

(add-stmt m n1:'(11))
;err:
Unable to add entry<n1:'(11)> due to constraint<(max 10 on ?0)>
at main: (add-stmt m n1:'(11))
;eof

(list-stmt m from n1:'(?x))
;=>'(n1:'(1) n1:'(10))

(remove-constraint m n1:'(1) (? on ?))
;=>'((max 10 on ?0))

(add-stmt m n1:'(11))
;=>true

(list-stmt m from n1:'(?x))
;=>'(n1:'(1) n1:'(10) n1:'(11))

