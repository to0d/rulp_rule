(new model m)
;=>m

(add-constraint m n1:'(?x) (min 5 on ?x))
;=>true

(add-stmt m n1:'(6))
;=>true

(add-stmt m n1:'(5))
;=>true

(add-stmt m n1:'(4))
;err:
Unable to add entry<n1:'(4)> due to constraint<(min 5 on ?0)>
at main: (add-stmt m n1:'(4))
;eof

(list-stmt m from n1:'(?x))
;=>'(n1:'(6) n1:'(5))

(remove-constraint m n1:'(1) (? on ?))
;=>'((min 5 on ?0))

(add-stmt m n1:'(1))
;=>true

(list-stmt m from n1:'(?x))
;=>'(n1:'(6) n1:'(5) n1:'(1))

