(list-stmt m)
;=>'(name1:'(a 2))

(add-constraint m name1:'(?x ?y) (>= 2 1))
;=>false

(add-constraint m name1:'(?x ?y) (< 2 1))
;err:
false expr constraint found: (< 2 1)
at main: (add-constraint m name1:'(?x ?y) (< 2 1))
;eof

