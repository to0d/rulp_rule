(new model m)
;=>m

(add-constraint m name1:'(?x ?y) (>= ?y (+ 1 1)))
;=>true

(add-stmt m name1:'(a 2))
;=>true

(add-stmt m name1:'(b 1))
;err:
Unable to add entry<name1:'(b 1)> due to constraint<(cmp-entry-value >= ?1 2)>
at main: (add-stmt m name1:'(b 1))
