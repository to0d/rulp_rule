(new model m)
;=>m

(defvar ?x false)
;=>&?x

(add-rule m if (var-changed ?x ?v true) '(?a ?b) do (-> '(?b ?a)) )
;=>RU000

(add-stmt m '(a b))
;=>true

(start m)
;=>2

(list-stmt m)
;=>'('(a b))

(setq ?x true)
;=>&?x

(start m)
;=>7

(list-stmt m)
;=>'('(a b) '(b a))

(setq ?x false)
;=>&?x

(add-stmt m '(x y))
;=>true

(start m)
;=>4

(list-stmt m)
;=>'('(a b) '(b a) '(x y))

(setq ?x true)
;=>&?x

(start m)
;=>7

(list-stmt m)
;=>'('(a b) '(b a) '(x y) '(y x))

