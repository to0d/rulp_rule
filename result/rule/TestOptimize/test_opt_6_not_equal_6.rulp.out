(new model m)
;=>m

(add-rule m if '(?a ?p ?b) (not (equal ?a ?b)) do (-> '(?b p2 ?a)))
;=>RU000

(add-rule m if '(?a2 ?p2 ?b2) '(?b2 ?p2 ?a2) (not (equal ?a2 ?b2)) do (-> '(?a2 p4 ?b2)))
;=>RU001

(add-stmt m '(a2 p2 b2))
;=>true

(start m)
;=>13

(state-of m)
;=>completed

(list-stmt m)
;=>'('(a2 p2 b2) '(b2 p2 a2) '(b2 p4 a2) '(a2 p4 b2))

