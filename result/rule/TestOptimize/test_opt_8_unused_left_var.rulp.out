(new model m)
;=>m

(add-rule m if '(?a p1 ?b) '(?b p2 c) do (-> '(?a p3 c)))
;=>RU000

(add-stmt m '(a p1 b1))
(add-stmt m '(a p1 b2))
(add-stmt m '(a p1 b3))
(add-stmt m '(b1 p2 c))
(add-stmt m '(b2 p2 c))
(add-stmt m '(b3 p2 c))
;=>true true true true true true

(start m)
;=>11

(state-of m)
;=>completed

(list-stmt m)
;=>'('(a p1 b1) '(a p1 b2) '(a p1 b3) '(b1 p2 c) '(b2 p2 c) '(b3 p2 c) '(a p3 c))

