(new model m)
;=>m

(add-rule m if '(?x p ?y) n1:'(p p2) do (-> m '(?y p ?x)))
;=>RU000

(add-stmt m '(a p c))
;=>true

(add-stmt m n1:'(p p2))
;=>true

(query-stmt m '(?x ?y) from '(?x p ?y))
;=>'('(a c) '(c a))

(remove-stmt m n1:'(p p2))
;=>'(n1:'(p p2))

(list-stmt m)
;=>'('(a p c))

(add-stmt m '(c p a))
;=>true

(start m)
;=>3

(list-stmt m)
;=>'('(a p c) '(c p a))

