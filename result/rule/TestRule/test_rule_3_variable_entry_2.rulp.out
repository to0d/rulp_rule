(new model m)
;=>m

(add-stmt m '(t1 t2 t3 t4))
;=>true

(add-stmt m '(t1 p t2))
;=>true

(add-stmt m '(t2 p t3))
;=>true

(add-rule m if '(?a ?b ?c ?d) '(?a p ?b) do (-> m '(?b ?c ?d ?a)) )
;=>RU000

(start m)
;=>10

(list-stmt m from '(?x1 ?x2 ?x3 ?x4))
;=>'('(t1 t2 t3 t4) '(t2 t3 t4 t1) '(t3 t4 t1 t2))

