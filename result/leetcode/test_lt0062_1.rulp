; f(m,n) = f(m-1,n) + f(m,n-1)
; f(m,n) = f(n,m)
; f(1,n) = 1

#define MAX_NUM 100
static int uniqueNum[MAX_NUM*MAX_NUM] = {0};

int uniquePaths(int m, int n) {
    
    if( m == 0 || n == 0 ) return 0;
    if( m < n )  return uniquePaths(n, m);
    if( n == 1 ) return 1;
    
    int num = uniqueNum[(m-1)*MAX_NUM+n-1];
    if( num == 0 )
    {   num = uniquePaths(m-1,n)+uniquePaths(m,n-1);
    }
    uniqueNum[(m-1)*MAX_NUM+n-1] = num;
    return num;
}