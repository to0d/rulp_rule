'(p x y) '(?a p ?b) '(?b p ?c) ; input
'('('(?a p ?b) '(p x y)) '(?b p ?c)) ; output
;expr: 
'(
    '(
        '(?a p ?b)
        '(p x y)
     )
    '(?b p ?c)
 )



;;---------------------------------------------------------;;
'(p x y) '(?a p ?b) '(?b p ?c) '(a b c) ; input
'('('('(?a p ?b) '(p x y)) '(?b p ?c)) '(a b c)) ; output
;expr: 
'(
    '(
        '(
            '(?a p ?b)
            '(p x y)
         )
        '(?b p ?c)
     )
    '(a b c)
 )



