'(?x ?y ?z) '(?a ?b ?c)

;;---------------------------------------------------------;;
'(?x ?y ?z) '(?a ?b ?c) (equal ?x ?a)

;;---------------------------------------------------------;;
'(?x ?y ?z) '(?a ?b ?c) (equal ?x ?a) (equal ?y ?b)

