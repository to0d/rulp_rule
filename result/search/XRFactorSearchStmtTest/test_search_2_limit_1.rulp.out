(new model m)
;=>m

(add-constraint m v1:'(?x) (type int on ?x))
;=>true

(add-constraint m v1:'(?x) (max 40 on ?x))
;=>true

(add-constraint m v1:'(?x) (min 37 on ?x))
;=>true

(add-constraint m v1:'(?x) (= (% ?x 2) 0))
;=>true

(defvar ?s (search m v2:'(?x) from v1:'(?x)))
;=>&?s

(state-of ?s)
;=>runnable

(start ?s limit 1)
;=>1

(state-of ?s)
;=>partial

(list-stmt m from v2:'(?x))
;=>'(v2:'(38))

