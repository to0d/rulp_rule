(new model m)
;=>m

(add-lazy-stmt m '(?n hasRelated ?key ?path) '('(a hasRelated k1 "path 1") '(a hasRelated k2 "path 2") '(a hasRelated k3 "path 3") '(b hasRelated k2 "path 4")))
;=>nil

(query-stmt m '(?n ?k ?p) from '(?n hasRelated ?k ?p))
;=>'('(a k1 "path 1") '(a k2 "path 2") '(a k3 "path 3") '(b k2 "path 4"))

