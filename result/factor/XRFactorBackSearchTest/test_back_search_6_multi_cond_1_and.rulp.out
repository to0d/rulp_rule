(new model m)
(add-rule m if '(?a p ?b) '(?b p ?c) do (-> m '(?a p ?c)))
(add-stmt m '(a p b))
(add-stmt m '(b p c))
;=>m RU000 true true

(setq ?bs-trace true)
;=>&?bs-trace

(back-search m (and '(a p c) '(a p b)) explain)
;=>'()
;out:
00000 SO0000: create_root, stmt=(and '(a p c) '(a p b)), type=STMT_OR, status=INIT
00000 SO0000: init begin, stmt=(and '(a p c) '(a p b)), type=STMT_OR, status=INIT
00000 SO0000: has stmt, stmt=(and '(a p c) '(a p b)), rst=false
00000 SO0000: not child, return false
00000 SO0000: init end, rst=false, status=COMPLETE, succ-child=null
00001 SO0000: has stmt, stmt=(and '(a p c) '(a p b)), rst=false
00001 SO0000: complete end, rst=false

;eof

(list-stmt m)
;=>'('(a p b) '(b p c))

