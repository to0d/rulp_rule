(new model m)
;=>m

(add-rule "RULE1" m if '(?x ?y ?z) '(?a ?b ?c) do (-> m '(?x ?y ?z ?a ?b ?c)))
;=>RULE1

(rete-node-of (get-rule m "RULE1") RRT_BETA0)
;=>'()

(rete-node-of (get-rule m "RULE1") RRT_BETA3)
;=>'('('(?0 ?1 ?2) '(?3 ?4 ?5)))

(add-stmt m '(a b c))
;=>true

(add-stmt m '(x y z))
;=>true

(start m)
;=>4

(state-of m)
;=>completed

(list-stmt m)
;=>'('(a b c) '(x y z) '(a b c a b c) '(x y z a b c) '(a b c x y z) '(x y z x y z))

