(load "rule")
;=>nil

(new model m)
(add-rule "R1" m if n1:'(?x) do (-> m n2:'(?x)))
(add-stmt m n1:'(a))
(add-stmt m n1:'(b))
(add-stmt m n1:'(c))
;=>m R1 true true true

(execute-rule (get-rule m "R1") '(n1:'(b)))
;=>2

(dump-status m (+ ?script-path ".status.1.txt"))
;=>nil

(list-stmt m)
;=>'(n1:'(b) n1:'(a) n1:'(c) n2:'(b))

(execute-rule (get-rule m "R1") '(n1:'(b)))
;=>0

(dump-status m (+ ?script-path ".status.2.txt"))
;=>nil

(list-stmt m)
;=>'(n1:'(b) n1:'(a) n1:'(c) n2:'(b))

