(new model m)
;=>m

(add-rule "R1" m if '(?x typeof c1) do (-> m '(?x typeof c2)) )
;=>R1

(add-rule "R2" m if '(?x p ?y) '(?y p ?z) do (-> m '(?x p ?z)) )
;=>R2

(add-stmt m '(a typeof c1))
;=>true

(add-stmt m '(a p b))
;=>true

(add-stmt m '(b p c))
;=>true

(start m)
;=>16

(state-of m)
;=>completed

(list-stmt m)
;=>'('(a typeof c1) '(a p b) '(b p c) '(a typeof c2) '(a p c))

